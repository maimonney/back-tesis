{
    "version": 2,
    "builds": [
      {
        "src": "api/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/app.js"
      },
      {
        "src": "/.well-known/vercel/microfrontend-routing",
        "status": 404
      }
    ]
  }
  